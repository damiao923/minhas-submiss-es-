import java.util.Scanner;
class Principal {
    public static void main(String[]args)  {
    Scanner sky = new Scanner(System.in);
    int A = sky.nextInt();
    int B = sky.nextInt();
    int C = sky.nextInt();
if(A != B && A != C) {
System.out.print("A");
}if( B != A && B != C)  {
System.out.print("B");
}if( C != A && C != B){
System.out.print("C");
}if( C == B && B == A) {
System.out.print("*");
}
 
     }
    
}
